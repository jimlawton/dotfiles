set bell-style none
set match-hidden-files off
"\eOA": history-search-backward
"\e[A": history-search-backward
"\eOB": history-search-forward
"\e[B": history-search-forward
"\eOC": forward-char
"\e[C": forward-char
"\eOD": backward-char
"\e[D": backward-char
"\e[3;5~": backward-kill-word
"\C-?": backward-kill-word
"\e(": kill-word
"\e[1;5D": backward-word
"\e[1;5C": forward-word

